Algoritmo Ej9Descuento
	Definir precioCompra Como Real;
	Definir precioFinal Como Real;
	
	Escribir "Ingrese precio total de la compra: ";
	Leer precioCompra;
	
	precioFinal<- precioCompra-(precioCompra/100)*15;
	
	Escribir "El precio final con el descuento del 15% aplicado es de ", precioFinal;

	
	
	
FinAlgoritmo
