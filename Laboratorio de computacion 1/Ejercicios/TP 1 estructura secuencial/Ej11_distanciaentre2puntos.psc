Proceso Ej11_distanciaentre2puntos
	Definir x1 Como Real;
	Definir x2 Como Real;
	Definir distancia Como Real;
	
	Escribir "ingrese el primer punto";
	Leer x1;
	Escribir "ingrese el segundo punto";
	Leer x2;
	
	distancia<-rc((x1-x2)*(x1-x2));
	
	Escribir "La distancia entre los dos puntos es de " , distancia;
	
FinProceso
