Proceso DistanciaDe2Numeros
	Definir distancia Como Real;
	Definir num1 Como Entero;
	Definir num2 Como Entero;
	
	Escribir "Ingrese el primer num";
	Leer num1;
	Escribir "Ingrese el segundo num";
	Leer num2;
	
	distancia<-rc((num1-num2)*(num1-num2));
	
	Escribir "la distancia entre los 2 numeros es de " , distancia;
	
	
FinProceso
